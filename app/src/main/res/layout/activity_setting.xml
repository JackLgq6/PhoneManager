<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <TextView
        style="@style/TitleStyle"
        android:text="设置中心"/>

    <com.eg.phonemanager.view.SettingItemView
        xmlns:phoneManager="http://schemas.android.com/apk/res/com.eg.phonemanager"
        android:id="@+id/siv_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        phoneManager:desTitle="自动更新设置"
        phoneManager:desOn="自动更新已开启"
        phoneManager:desOff="自动更新已关闭">
    </com.eg.phonemanager.view.SettingItemView>
    <com.eg.phonemanager.view.SettingItemView
        xmlns:phoneManager="http://schemas.android.com/apk/res/com.eg.phonemanager"
        android:id="@+id/siv_homeLocation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        phoneManager:desTitle="电话归属地显示设置"
        phoneManager:desOn="归属地显示已开启"
        phoneManager:desOff="归属地显示已关闭">
    </com.eg.phonemanager.view.SettingItemView>

</LinearLayout>